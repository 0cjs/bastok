from tokmsbasic.tokentext  import TokenText

TEST_TEXT = b''.join([
    b'\xFF',                        # file type header
    b'\x11\x80', b'\x0A\x00',       # next line pointer, line 10
    b'\x8F Tokenized\x00',          # REM Tokenized
    b'\x1B\x80', b'\x14\x00',       # next line pointer, line 10
    b'\x91 "x"\x00'                 # PRINT "x"
    b'\x00\x00',                    # terminator
])

def test_tokentext():
    tt = TokenText(TEST_TEXT)
    assert TEST_TEXT == tt.orig_text
   #assert TEST_TEXT[1:] == tt.text()
